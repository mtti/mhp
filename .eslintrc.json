{
    "extends": "airbnb-base",
    "env": {
      "mocha": true
    },
    "globals": {
      "assert": true
    },
    "rules": {
      "import/no-extraneous-dependencies": ["error", {"devDependencies": ["test/**/*.js", "**/*.spec.js"]}],
      "import/no-unresolved": [2, {"caseSensitive": false}]
    }
}
