{
  "extends": "airbnb-base",
  "env": {
      "mocha": true
  },
  "globals": {
      "assert": true,
      "sandbox": true
  },
  "rules": {
      "import/no-extraneous-dependencies": ["error", {"devDependencies": ["src/test/**/*.js", "src/**/*.spec.js"]}],
      "import/no-unresolved": [2, {"caseSensitive": false}],
      "no-underscore-dangle": "off",
      "prefer-destructuring": "off"
  }
}
